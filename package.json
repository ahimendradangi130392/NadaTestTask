        "@mui/material": "^5.11.0",
        "@sentry/react": "^7.20.0",
        "@sentry/tracing": "^7.20.0",
        "antd": "^4.21.4",
        "axios": "^0.27.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-query": "^3.39.2",
        "react-router-dom": "^6.4.3",
        "react-scripts": "^5.0.1",
        "styled-components": "^5.3.5",
        "web-vitals": "^2.1.4"
